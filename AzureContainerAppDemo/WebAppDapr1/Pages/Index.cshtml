@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
    @foreach (var forecast in Model.WeatherForecast)
    {
        <p>Date: @forecast.Date, TemperatureC: @forecast.TemperatureC, TemperatureF: @forecast.TemperatureF, Summary: @forecast.Summary</p>
    }
</div>
